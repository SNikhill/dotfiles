# Alias for FAST git usage.
# Contains alias for common git commands I use

alias ga="git add -A"
alias gc="git commit"
alias gc!="git commit --amend"

alias gb="git branch"
alias gl="git log"
alias gs="git status"

alias gR="git restore"
alias gre="git restore"

alias gp="git push"
alias gpF="git push --force-with-lease"
alias gf="git fetch --prune"

alias gr="git rebase"
alias grb="git rebase"

alias gch="git checkout"
